<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">
	<changeSet id="GNUOB ribelli objecten" author="Bernard Arjan Draaisma" context="ribelli" runOnChange="true">
		<sql>SET FOREIGN_KEY_CHECKS = 0;</sql>
		<delete tableName="GNUOB_ACCESS" />
		<delete tableName="GNUOB_GROUPS" />
		<delete tableName="GNUOB_PERMISSIONS" />
		<delete tableName="GNUOB_SETTINGS" />
		<delete tableName="GNUOB_SITES" />
		<delete tableName="GNUOB_USERS" />
		<delete tableName="GNUOB_USERS_GNUOB_GROUPS" />
		<delete tableName="GNUOB_USERS_GNUOB_SITES" />
		<delete tableName="GNUOB_ROLES" />
		<loadData tableName="gnuob_permissions" file="src/main/resources/liquibase/ribelli/GNUOB_PERMISSIONS.csv">
			<column name="ID" type="NUMERIC" />
			<column name="VERSION" type="NUMERIC" />
			<column name="OWNER" type="STRING" />
			<column name="GROUP" type="STRING" />
			<column name="OTHERS" type="STRING" />
		</loadData>
		<loadData tableName="gnuob_access" file="src/main/resources/liquibase/ribelli/GNUOB_ACCESS.csv">
			<column name="ID" type="NUMERIC" />
			<column name="VERSION" type="NUMERIC" />
			<column name="ACTIVE" type="BOOLEAN" />
			<column name="group_ID" type="NUMERIC" />
			<column name="permission_ID" type="NUMERIC" />
			<column name="site_ID" type="NUMERIC" />
			<column name="owner_ID" type="NUMERIC" />
		</loadData>
		<loadData tableName="gnuob_users" file="src/main/resources/liquibase/ribelli/GNUOB_USERS.csv">
			<column name="ID" type="NUMERIC" />
			<column name="DESCRIPTION" type="STRING" />
			<column name="NAME" type="STRING" />
			<column name="PASSWORD" type="STRING" />
			<column name="ACCESS" type="STRING" />
			<column name="ROOT" type="BOOLEAN" />
		</loadData>
		<loadData tableName="gnuob_groups" file="src/main/resources/liquibase/ribelli/GNUOB_GROUPS.csv">
			<column name="ID" type="NUMERIC" />
			<column name="DESCRIPTION" type="STRING" />
			<column name="NAME" type="STRING" />
			<column name="RULE" type="STRING" />
		</loadData>
		<loadData tableName="gnuob_sites" file="src/main/resources/liquibase/ribelli/GNUOB_SITES.csv">
			<column name="ID" type="NUMERIC" />
			<column name="DESCRIPTION" type="STRING" />
			<column name="NAME" type="STRING" />
		</loadData>
		<loadData tableName="gnuob_users_gnuob_sites" file="src/main/resources/liquibase/ribelli/GNUOB_USERS_GNUOB_SITES.csv">
			<column name="sites_ID" type="NUMERIC" />
			<column name="GNUOB_USERS_ID" type="NUMERIC" />
		</loadData>
		<loadData tableName="gnuob_users_gnuob_groups" file="src/main/resources/liquibase/ribelli/GNUOB_USERS_GNUOB_GROUPS.csv">
			<column name="groups_ID" type="NUMERIC" />
			<column name="GNUOB_USERS_ID" type="NUMERIC" />
		</loadData>
		<loadData tableName="gnuob_settings" file="src/main/resources/liquibase/ribelli/GNUOB_SETTINGS.csv">
			<column name="ID" type="NUMERIC" />
			<column name="VALUE" type="STRING" />
			<column name="PROPERTY" type="STRING" />
			<column name="DESCRIPTION" type="STRING" />
		</loadData>
		<loadData tableName="gnuob_roles" file="src/main/resources/liquibase/ribelli/GNUOB_ROLES.csv">
			<column name="GNUOB_USERS_ID" type="NUMERIC" />
			<column name="ROLE" type="STRING" />
		</loadData>
		<rollback>
			<sql>SET FOREIGN_KEY_CHECKS = 0;</sql>
			<delete tableName="GNUOB_ACCESS" />
			<delete tableName="GNUOB_GROUPS" />
			<delete tableName="GNUOB_PERMISSIONS" />
			<delete tableName="GNUOB_SETTINGS" />
			<delete tableName="GNUOB_SITES" />
			<delete tableName="GNUOB_USERS" />
			<delete tableName="GNUOB_USERS_GNUOB_GROUPS" />
			<delete tableName="GNUOB_USERS_GNUOB_SITES" />
			<delete tableName="GNUOB_ROLES" />
			<sql>SET FOREIGN_KEY_CHECKS = 1;</sql>
		</rollback>
	</changeSet>
</databaseChangeLog>

